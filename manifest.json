{
  "manifest_version" : 2,
  "name": "SocialMedia",
  "description": "This extension brings all your social media together on the new tab page",
  "version": "1.0",

   "browser_action": {
    /*"default_icon": "icon.png", */
    "default_popup": "popup.html"
  },
 "background" :
  { "persistent" : false,
  	"scripts" : ["background.js"]
  },

  "chrome_url_overrides" : {
    "newtab": "socialmedia.html"
  }

}